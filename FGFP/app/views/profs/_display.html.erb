			<div class="media-content">
				<% @profs.each do | prof | %>
					<div class="box">
						<article class="media">
							<div class="media-top">
								<figure class="image is-64x64">
									<img src="https://bulma.io/images/placeholders/64x64.png"> 
								</figure>
							</div>
							<div class="media-content">
								<div class="content">
				 					<strong>Sample Name</strong><br />
				 				</div>
								<div class="content">
									<small>Department</small><br /> 
									<small>Office</small><br /> 
									<small>email</small><br /> 
								</div>


								<!-- not sure what this does -->
								<nav class="level">
									<div class="level-left is-mobile">
										<%= link_to prof, class: "level-item" do %>
											<span class="icon">
												<i class="fa fa-link" aria-hidden="true"></i>
											</span>
										<% end %>

									</div>
								</nav>



							</div> 
						</article>
					</div>
				<% end %>
			</div>
